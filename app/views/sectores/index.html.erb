   <h1>Sectores</h1>
   <section>
      <ul class="listaSect">
        <% @sectores.each do |sector| %>
          <li class="sectCont">
            <div class="seComuNombre"><p><%= sector.comunidad.nombre %></p></div>
            <div class="sectNom"><p><%= sector.nombre %></p></div>
            <ul>
              <li><%= link_to 'Ver', sector %></li>
              <li><%= link_to 'Editar', edit_sector_path(sector) %></li>
              <li><%= link_to 'Eliminar', sector, method: :delete, data: { confirm: 'EstÃ¡ seguro?' } %></li>
            </ul>
          </li>
        <%end%>
      </ul>
      <div class="nuevoSect">
        <%= link_to 'Crear Sector', new_sector_path %>
      </div>
    </section>

