<h1>Listing comunidades_users</h1>

<table>
  <thead>
    <tr>
      <th>Comunidad</th>
      <th>User</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @comunidades_users.each do |comunidad_user| %>
      <tr>
        <td><%= comunidad_user.comunidad_id %></td>
        <td><%= comunidad_user.user_id %></td>
        <td><%= link_to 'Show', comunidad_user %></td>
        <td><%= link_to 'Edit', edit_comunidad_user_path(comunidad_user) %></td>
        <td><%= link_to 'Destroy', comunidad_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

