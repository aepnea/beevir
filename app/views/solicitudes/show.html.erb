<% if session[:comunidad_id].to_s == @solicitud.comunidad_id.to_s %>

  <p id="notice"><%= notice %></p>

  <p>
     <strong>Titulo:</strong>
     <%= @solicitud.titulo %>
  </p>

  <p>
     <strong>Descripcion:</strong>
     <%= @solicitud.descripcion %>
  </p>

  <p>
     <strong>Adjunto:</strong>
     <%= @solicitud.adjunto %>
  </p>

  <p>
     <strong>Comunidad:</strong>
     <%= @solicitud.comunidad.nombre %>
  </p>

  <p>
     <strong>Unidad:</strong>
     <%= @solicitud.unidad.numero %>
  </p>

  <p>
     <strong>Estado:</strong>
     <%= @solicitud.estado.estado %>
  </p>

  <p>
     <strong>Role:</strong>
     <%= @solicitud.role.name %>
  </p>

  <%= link_to 'Editar', edit_solicitud_path(@solicitud) %> |
  <%= link_to 'Volver', solicitudes_path %>
<%else%>
  <h1>Invalid Url</h1>
<%end%>